{
    "name": "modera/notification-bundle",
    "type": "symfony-bundle",
    "description": "Provides functionality to store/retrieve notifications.",
    "license": "MIT",
    "conflict": {
        "doctrine/persistence": "<1.3"
    },
    "require": {
        "php": ">=7.4",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0",
        "modera/expander-bundle": "^5.0|^6.0"
    },
    "require-dev": {
        "phake/phake": "^2.0",
        "phpunit/phpunit": "^5.5",
        "symfony/security": "^5.4",
        "modera/foundation-bundle": "^5.0"
    },
    "authors": [
        {
            "name": "Sergei Lissovski"
        },
        {
            "name": "Sergei Vizel"
        }
    ],
    "autoload": {
        "psr-0": { "Modera\\NotificationBundle": "" }
    },
    "target-dir": "Modera/NotificationBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "modera-module": {
            "register-bundle": "Modera\\NotificationBundle\\ModeraNotificationBundle"
        }
    }
}
